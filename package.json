    "lint:css": "stylelint src/**/*.scss"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/samma89/React-Typescript-Parcel-Starter.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/samma89/React-Typescript-Parcel-Starter/issues"
  },
  "homepage": "https://github.com/samma89/React-Typescript-Parcel-Starter#readme",
  "devDependencies": {
    "@types/react": "^16.4.7",
    "@types/react-dom": "^16.0.6",
    "cssnano": "^4.0.4",
    "node-sass": "^4.9.2",
    "parcel-bundler": "^1.9.7",
    "pre-commit": "^1.2.2",
    "stylelint": "^9.4.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-scss": "^3.2.0",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.9.2",
    "tslint-config-prettier": "^1.14.0",
