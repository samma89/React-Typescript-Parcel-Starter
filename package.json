{
  "name": "react-typescript-parcel-starter",
  "version": "1.0.0",
  "description": "Yet another React, Typescript, Parcel boilerplate project with some extra features",
  "main": "index.js",
  "scripts": {
    "start": "parcel src/index.html",
    "build": "parcel build src/index.html -d dist",
    "lint": "npm run lint:js && npm run lint:css",
